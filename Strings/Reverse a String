import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc =  new Scanner(System.in);
		System.out.print("Enter the string :  ");
		String st = sc.nextLine();
		int s = st.length();
		String tra = "";
		
		for (int i=0;i<s;i++){
		    char ch = st.charAt(i);//extract the each character from the string 
		    tra = ch+tra;// insert the extracted char into the new char
		}
		System.out.println("Reverse string : "+tra);
	}
}
